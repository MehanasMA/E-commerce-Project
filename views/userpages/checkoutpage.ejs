									<h4 class="mb-3 mt-5">Billing address</h4>
									<hr>
									<div class="row">
										<div class="col-md-6 mb-3">
									
											<table>
												<thead>
													<tr>
														<th></th>


													</tr>

												</thead>
												<tbody>
													<% let i=0; for(add of useraddress) { %>
														<tr>
															<th>
																<label class="control control-checkbox">
																	<input type="radio"
																		value="<%= add.name%>, <%=add.address%>,<%=add.district%>,<%=add.statePlace%>,<%=add.pincode%> ,<%=add.email%>,<%=add.mobile %>"
																		name="address" />

																	<div class="control_indicator p-3"></div>



																</label>
