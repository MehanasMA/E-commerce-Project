																				<i class="material-icons">delete</i>
																			</button>
															</td>
															<br>
															<td></td>
														</tr>
														<%i++;}%>
													</div>
													</tbody>
												</table>
											
										<div class="btn btn-primary"><a href="/addAddress">Add New Address</div>

									</div>
								</div>
							</div>
							<!-- </div> -->
