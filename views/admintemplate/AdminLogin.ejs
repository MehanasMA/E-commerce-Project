<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- CSS only -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
  <title>AdminLogin</title>
</head>

<body class="col d-flex justify-content-center mt-5">

  <form action="/admin/adminloginpost" method="post">
    <div class="text-center fw-bold mt-5 fs-1">Admin</div>
    <!-- Email input -->
    <div class="form-outline mb-4 mt-5">
      <label class="form-label" for="form2Example1">Email address</label>
      <input type="email" name="Email" id="form2Example1" class="form-control" required>

    </div>

    <!-- Password input -->
    <div class="form-outline mb-4">
      <label class="form-label" for="form2Example2">Password</label>
      <input type="password" name="Password" id="form2Example2" class="form-control" required>

    </div>

    <div class="col">
      <!-- Simple link -->
      <a href="#!">Forgot password?</a>
    </div>


    <!-- Submit button -->
    <button type="submit" class="btn btn-primary btn-block mb-4 ms-5">Sign in</button>

  </form>



  <!-- JavaScript Bundle with Popper-->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
    crossorigin="anonymous"></script>
</body>

</html>